# Supabase Configuration
SUPABASE_URL=TODO_YOUR_SUPABASE_PROJECT_URL
SUPABASE_SERVICE_ROLE_KEY=TODO_YOUR_SUPABASE_SERVICE_ROLE_KEY
NEXT_PUBLIC_SUPABASE_URL=TODO_YOUR_SUPABASE_PROJECT_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=TODO_YOUR_SUPABASE_ANON_KEY

# Feature Flags
NEXT_PUBLIC_FEATURE_DEDUPE_V2=true

# Pricing Feature Flags (Phase 1: Engagement-First Pricing)
# NEXT_PUBLIC_FEATURE_PRICE_V2 - Server-side: Attach normalizedPrice to event data
NEXT_PUBLIC_FEATURE_PRICE_V2=false

# NEXT_PUBLIC_FEATURE_ENGAGEMENT_PRICING - Client-side: Use PriceBadge component in UI
NEXT_PUBLIC_FEATURE_ENGAGEMENT_PRICING=false

# NEXT_PUBLIC_PRICE_FALLBACK_LABEL - A/B test for unknown pricing label
# Options: "tickets" | "paid" | "see_pricing"
# tickets → "Tickets Available →" (default - highest CTR)
# paid → "Paid" (baseline - lowest CTR)
# see_pricing → "See Pricing →" (action-oriented)
NEXT_PUBLIC_PRICE_FALLBACK_LABEL=tickets

# User Interaction Tracking
NEXT_PUBLIC_FEATURE_TRACKING_V1=false

# Personalized Rails (requires TRACKING_V1)
# Max personal rails to show (3-5 recommended)
NEXT_PUBLIC_PERSONALIZED_RAILS_MAX=3
# Min events per rail to show (inventory threshold)
NEXT_PUBLIC_PERSONALIZED_RAILS_MIN_EVENTS=4
# Min interactions required before showing personal rails
NEXT_PUBLIC_PERSONALIZED_RAILS_MIN_INTERACTIONS=5
# Discovery floor: % of events that must be from general categories (0.0-1.0)
NEXT_PUBLIC_PERSONALIZED_DISCOVERY_FLOOR=0.3
# Veto threshold: hide events with this many downvotes (unless re-probe)
NEXT_PUBLIC_PERSONALIZED_VETO_THRESHOLD=2
# Enable personalized rails
NEXT_PUBLIC_FEATURE_PERSONALIZED_RAILS=false

# Seen Store - Prevent showing same events repeatedly
# TTL for seen events in days
NEXT_PUBLIC_SEEN_STORE_TTL_DAYS=14
# Enable seen store filtering
NEXT_PUBLIC_FEATURE_SEEN_STORE=false

# Dynamic Category Manager - Auto-spawn/sunset rails
# Max core (static) category rails to show
NEXT_PUBLIC_DYNAMIC_CORE_LIMIT=10
# Max dynamic (spawned) category rails to show
NEXT_PUBLIC_DYNAMIC_RAILS_LIMIT=5
# Min affinity score to spawn new rail (0.0-1.0)
NEXT_PUBLIC_DYNAMIC_SPAWN_THRESHOLD=0.4
# Days of inactivity before sunsetting rail
NEXT_PUBLIC_DYNAMIC_SUNSET_DAYS=7
# Enable dynamic category manager
NEXT_PUBLIC_FEATURE_DYNAMIC_CATEGORIES=false

# ============================================================
# UX WIRING MVP - Saved, Search, Thumbs, Filters, Sidebar
# ============================================================

# Saved Events - Local bookmark system
NEXT_PUBLIC_FEATURE_SAVED_EVENTS=false

# Search - Enhanced search with highlighting
NEXT_PUBLIC_FEATURE_ENHANCED_SEARCH=false

# Thumbs - Vote up/down for events (integrates with veto/affinity)
NEXT_PUBLIC_FEATURE_THUMBS=false

# Filters - Price, date, category filtering
NEXT_PUBLIC_FEATURE_FILTERS=false

# Enhanced Sidebar - Saved events, filters, search in sidebar
NEXT_PUBLIC_FEATURE_ENHANCED_SIDEBAR=false

# ============================================================
# DETERMINISTIC DAILY SHUFFLE & CACHING
# ============================================================

# Deterministic daily shuffle - stable order per day
NEXT_PUBLIC_FEATURE_DAILY_SHUFFLE=true

# Client-side event caching with background refresh
NEXT_PUBLIC_FEATURE_CACHED_EVENTS=true

# Daily shuffle seed scope (city or global)
DAILY_SHUFFLE_SEED_SCOPE=city

# Cache TTL in minutes (default: 30)
CACHE_TTL_MINUTES=30

# Max items per cache entry (default: 2000)
CACHE_MAX_ITEMS=2000

# Seen events TTL in days (already defined above, kept for reference)
# NEXT_PUBLIC_SEEN_STORE_TTL_DAYS=14

# Web Push VAPID Keys (Generate with: npx web-push generate-vapid-keys)
VAPID_PUBLIC_KEY=TODO_YOUR_VAPID_PUBLIC_KEY
VAPID_PRIVATE_KEY=TODO_YOUR_VAPID_PRIVATE_KEY
NEXT_PUBLIC_VAPID_PUBLIC_KEY=TODO_YOUR_VAPID_PUBLIC_KEY

# Stripe Configuration (Optional for now)
STRIPE_SECRET_KEY=TODO_YOUR_STRIPE_SECRET_KEY
STRIPE_WEBHOOK_SECRET=TODO_YOUR_STRIPE_WEBHOOK_SECRET
NEXT_PUBLIC_STRIPE_PRICE_ID=TODO_YOUR_STRIPE_RECURRING_PRICE_ID
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=TODO_YOUR_STRIPE_PUBLISHABLE_KEY

# Email Service (Optional for now)
RESEND_API_KEY=TODO_YOUR_RESEND_API_KEY

# App Configuration
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# API Keys for External Services
# Ticketmaster
TICKETMASTER_API_KEY=TODO_YOUR_TICKETMASTER_API_KEY
TICKETMASTER_API_SECRET=TODO_YOUR_TICKETMASTER_API_SECRET

# Eventbrite
EVENTBRITE_API_KEY=TODO_YOUR_EVENTBRITE_PRIVATE_TOKEN

# Songkick
SONGKICK_API_KEY=TODO_YOUR_SONGKICK_API_KEY

# Meetup
MEETUP_API_KEY=TODO_YOUR_MEETUP_API_KEY

# Google Places
GOOGLE_PLACES_API_KEY=TODO_YOUR_GOOGLE_PLACES_API_KEY

# Yelp
YELP_API_KEY=TODO_YOUR_YELP_API_KEY

# OpenAI (for ML features)
OPENAI_API_KEY=TODO_YOUR_OPENAI_API_KEY

# Environment
NODE_ENV=development